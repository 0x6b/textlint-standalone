{
  "rules": {
    "@textlint-ja/preset-ai-writing": true,
    "preset-ja-technical-writing": true,
    "@0x6b/no-emoji": true,
    "@0x6b/no-emphasis": true,
    "@0x6b/no-hr-before-heading": true,
    "@0x6b/no-numbered-headings-and-bullets": true,
    "@0x6b/no-smart-quotes": true,
    "@0x6b/normalize-whitespaces": true
  }
}
